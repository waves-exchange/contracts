{
  "env": {
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-undef": "off",
    "no-console": "off",
    "import/extensions": "off",
    "no-restricted-syntax": "off",
    "func-names": "off",
    "import/prefer-default-export": "off",
    "no-underscore-dangle": "off",
    "no-unused-expressions": "off"
  },
  "ignorePatterns": [
    "archive/",
    "deprecated/"
  ]
}
